{
  "name": "org-boilerplate",
  "version": "0.0.0",
  "private": true,
  "engines": {
    "node": ">=8.16",
    "npm": ">=6.4.1"
  },
  "browserslist": [
    ">1%",
    "last 4 versions",
    "Firefox ESR",
    "not ie < 9"
  ],
  "dependencies": {
    "classnames": "2.2.6",
    "core-js": "3.1.4",
    "express": "4.17.1",
    "history": "4.9.0",
    "isomorphic-style-loader": "4.0.0",
    "load-script": "1.0.0",
    "normalize.css": "7.0.0",
    "object-assign": "4.1.1",
    "pretty-error": "2.1.1",
    "query-string": "^5.1.1",
    "react": "16.8.6",
    "react-dom": "16.8.6",
    "react-style-proptype": "3.2.2",
    "regenerator-runtime": "0.13.2",
    "source-map-support": "0.4.18",
    "universal-router": "3.2.0"
  },
  "devDependencies": {
    "@babel/preset-flow": "^7.0.0",
    "assets-webpack-plugin": "^3.9.10",
    "autoprefixer": "^7.1.1",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.1.1",
    "babel-loader": "^7.1.1",
    "babel-plugin-rewire": "^1.1.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-react-optimize": "^1.0.1",
    "babel-preset-stage-2": "^6.24.1",
    "babel-register": "^6.24.1",
    "babel-template": "^6.25.0",
    "babel-types": "^6.25.0",
    "browser-sync": "^2.26.5",
    "chokidar": "^1.7.0",
    "cross-env": "^5.0.1",
    "css-loader": "^0.28.4",
    "enzyme": "^2.9.1",
    "eslint": "^4.4.1",
    "eslint-config-airbnb": "^15.0.1",
    "eslint-config-prettier": "^2.3.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-import-resolver-node": "^0.3.2",
    "eslint-loader": "^1.8.0",
    "eslint-plugin-flowtype": "^2.34.1",
    "eslint-plugin-import": "^2.6.1",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-postcss-modules": "^1.1.2",
    "eslint-plugin-prettier": "^2.1.2",
    "eslint-plugin-react": "^7.0.1",
    "file-loader": "^3.0.1",
    "flow-bin": "^0.104.0",
    "front-matter": "^2.1.2",
    "glob": "^7.1.2",
    "husky": "^0.14.1",
    "identity-obj-proxy": "^3.0.0",
    "jscodeshift": "^0.7.0",
    "lint-staged": "^4.0.0",
    "markdown-it": "^8.3.1",
    "mkdirp": "^0.5.1",
    "null-loader": "^0.1.1",
    "opn-cli": "^3.1.0",
    "pixrem": "^4.0.1",
    "pleeease-filters": "^4.0.0",
    "postcss": "^6.0.4",
    "postcss-calc": "^6.0.0",
    "postcss-color-function": "^4.0.0",
    "postcss-custom-media": "^6.0.0",
    "postcss-custom-properties": "^6.1.0",
    "postcss-custom-selectors": "^4.0.1",
    "postcss-flexbugs-fixes": "^3.0.0",
    "postcss-import": "^10.0.0",
    "postcss-loader": "^3.0.0",
    "postcss-media-minmax": "^3.0.0",
    "postcss-nested": "^2.0.2",
    "postcss-nesting": "^4.0.1",
    "postcss-pseudoelements": "^5.0.0",
    "postcss-selector-matches": "^3.0.1",
    "postcss-selector-not": "^3.0.1",
    "prettier": "^1.5.2",
    "prop-types": "^15.7.2",
    "raw-loader": "^0.5.1",
    "react-deep-force-update": "^2.0.1",
    "react-error-overlay": "^1.0.9",
    "react-hot-loader": "^3.1.3",
    "react-test-renderer": "^15.6.1",
    "redux-mock-store": "^1.2.3",
    "rimraf": "^2.6.1",
    "serverless-s3-deploy": "^0.9.0",
    "serverless-scriptable-plugin": "^1.0.4",
    "sinon": "^3.2.0",
    "stylelint": "^8.0.0",
    "stylelint-config-prettier": "^5.2.0",
    "stylelint-config-standard": "^17.0.0",
    "stylelint-order": "^0.6.0",
    "svg-url-loader": "^2.3.2",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "url-loader": "^0.5.9",
    "webpack": "^4.30.0",
    "webpack-bundle-analyzer": "^3.3.2",
    "webpack-dev-middleware": "^3.6.2",
    "webpack-hot-middleware": "^2.18.0",
    "webpack-node-externals": "^1.6.0"
  },
  "babel": {
    "plugins": [],
    "presets": [
      [
        "env",
        {
          "targets": {
            "node": "current"
          }
        }
      ],
      "flow",
      "stage-2",
      "react"
    ],
    "env": {
      "test": {
        "plugins": [
          "rewire"
        ]
      }
    },
    "ignore": [
      "/node_modules/",
      "/build/chunks/",
      "/build/public/",
      "/build/updates/",
      "/build/server.js"
    ]
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "eslint --fix",
      "git add"
    ],
    "*.{json,graphql}": [
      "prettier --write",
      "git add"
    ],
    "*.{css,less,scss,sss}": [
      "stylelint --fix",
      "git add"
    ]
  },
  "scripts": {
    "precommit": "lint-staged",
    "lint-js": "eslint --ignore-pattern \"!**/.*\" .",
    "lint-css": "stylelint \"src/**/*.{css,less,scss,sss}\"",
    "lint": "npm run lint-js && npm run lint-css && npm run flow",
    "flow": "flow",
    "fix-js": "npm run lint-js -- --fix",
    "fix-css": "npm run lint-css -- --fix",
    "fix": "npm run fix-js && npm run fix-css",
    "build": "babel-node tools/run build",
    "start": "babel-node tools/run start"
  }
}
